
@{
    ViewBag.Title = "Index";

}

<script>
    function DisplayImages() {
        $("#FirstSection").fadeToggle(1000);
        $("#SecondSection").fadeToggle(1010);
        $("#ThirdSection").fadeToggle(1020);
    }
</script>
<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous" />
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

<main>
    @Html.Raw(TempData["Login"])
    <center>
        <section class="row" aria-labelledby="aspnetTitle">
            <h1 id="title" style="margin-top:30px">Welcome to ABC Hotel</h1>
            <p class="lead">Book your hotel rooms online now!!</p>
            <p><button class="btn btn-outline-dark" onclick="DisplayImages()">MORE &raquo;</button></p>

        </section>

    </center>
    <br />
    <div class="row">
        <section class="col-md-4" aria-labelledby="gettingStartedTitle" id="FirstSection" style="display:none">
            <h2 id="gettingStartedTitle">Affordable Rooms</h2>
            <p>
                <img src="~/images/img1.jpg" alt="image1" style="width: 350px; height: 300px" />
            </p>
        </section>

        <section class="col-md-4" aria-labelledby="librariesTitle" id="SecondSection" style="display:none">
            <h2 id="librariesTitle">Great Service</h2>
            <p>
                <img src="~/images/img2.jpg" alt="image2" style="width: 350px; height: 300px" />
            </p>
        </section>
        <section class="col-md-4" aria-labelledby="hostingTitle" id="ThirdSection" style="display:none">
            <h2 id="hostingTitle">With mesmerising views</h2>
            <p id="img3">
                <img src="~/images/img3.jpg" alt="image3" style="width: 350px; height: 300px" />
            </p>
        </section>
    </div>

